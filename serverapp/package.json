    "moment": "^2.24.0",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.43.2",
    "sequelize-cli": "^5.4.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.6",
