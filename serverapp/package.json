{
  "name": "typescript-app",
  "version": "1.0.0",
  "description": "TypeScript Practice App",
  "main": "server.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./build",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
    "start": "node ./build/server.js",
    "prod": "npm run build && npm run start",
    "test": "NODE_ENV=test nyc mocha --exit -r ts-node/register src/**/*.test.ts",
    "create-db": "run-script-os",
    "create-db:win32": "cd .. && cd postgres/ && docker-compose.exe up -d && cd .. && cd serverapp",
    "create-db:darwin:linux": "cd .. && cd postgres/ && docker-compose up -d && cd .. && cd serverapp",
    "delete-db": "docker rm finance_postgres -f",
    "setup-db": "node demo.js",
    "start-db": "docker start finance_postgres",
    "stop-db": "docker stop finance_postgres",
    "show-db": "docker ps -a"
  },
  "author": "Usman Anwar",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "run-script-os": "^1.0.5",
    "sequelize": "^4.43.2",
    "sequelize-cli": "^5.4.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.8",
    "@types/sequelize": "^4.27.24",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "faker": "^4.1.0",
    "mocha": "^10.2.0",
    "nodemon": "^1.19.0",
    "nyc": "^14.1.0",
    "rimraf": "^2.6.3",
    "standard": "^12.0.1",
    "ts-node-dev": "^1.0.0-pre.32"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "all": true
  }
}
