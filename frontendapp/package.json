{
  "name": "account-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "lib:build": "ng build lpToolkit && npm run postbuild",
    "postbuild": "copyfiles --up 6 packages/lp-toolkit/src/lib/assets/styles/* dist/lp-toolkit/styles/",
    "lib:publish": "cd packages/lp-toolkit && npm version patch && cd ../.. && npm run lib:build && cd dist/lp-toolkit && git init && git remote add origin git@github.com:LightPointFinancialTechnology/lpToolkit.git && git add -A && git commit -m \"deploy commit\" && git push origin master --force",
    "deploy": "copyfiles dist/AccountApp/**/* ../distribution/XA/Web/server/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "ag-grid-angular": "^22.0.0",
    "ag-grid-community": "22.0.0",
    "ag-grid-enterprise": "22.0.0",
    "angular-split": "^3.0.0",
    "bootstrap": "^4.3.1",
    "build": "^0.1.4",
    "core-js": "^3.4.1",
    "font-awesome": "^4.7.0",
    "intl": "^1.2.5",
    "lp-toolkit": "git+https://github.com/LightPointFinancialTechnology/lpToolkit.git",
    "moment": "^2.24.0",
    "ngcatalyst": "^1.0.12",
    "ngx-bootstrap": "^5.2.0",
    "ngx-daterangepicker-material": "^2.1.5",
    "ngx-toastr": "^11.2.1",
    "request": "^2.88.0",
    "run": "^1.4.0",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.19",
